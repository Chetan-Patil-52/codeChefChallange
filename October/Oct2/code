#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
    int t;
    cin >> t;
    while(t--){
        int a,b,ans,c;
        cin >> a >> b;
        c = b-a;
        if(c%8==0){
            ans = c/8;
        }else{
            ans = c/8 + 1;
        }
        cout << ans << endl;
    }
}
