#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
    int t;
    cin >> t;
    while(t--){
        int a,b;
        cin >> a >> b;
        int x = a + b;
        int y = x + 1;
        if(y%4==0 || y%4==3){
            cout << "Bob" << endl;
        }else{
            cout << "Alice" << endl;
        }
    }
}
