#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
    int t;
    cin >> t;
    while(t--){
        int x;
        cin >> x;
        if(x%5==0 || x%10==0){
            int count_10 = x/10;
            int rem = x %10;
            int count_5 = rem/5;
            cout << count_5+count_10<<endl;
        }else{
            cout << -1 << endl;
        }
    }
}
